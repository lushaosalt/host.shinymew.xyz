server {

	listen 80;
	listen [::]:80;
	
	root /var/www/SearchTLD;

	index index.html index.htm index.php;

	server_name sample;

	location / {
		try_files $uri $uri/ =404;
	}

	location ~ \.php$ {
		include snippets/fastcgi-php.conf;
	
		fastcgi_pass unix:/run/php/php7.4-fpm.sock;
	}
	
	location ~ /\.ht {
		deny all;
	}


}
